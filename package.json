{
  "name": "ctng-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "add": "ng g lib --prefix=ct",
    "build": "ng build",
    "build:app": "ng build --prod",
    "build:watch": "./scripts/build.sh --watch",
    "build:libs": "./scripts/build.sh",
    "build:libs:release": "./scripts/build.sh --release",
    "browser": "npx browserslist",
    "test": "ng test",
    "test:app": "ng test ctng-app",
    "test:libs": "./scripts/test.sh",
    "lint": "ng lint",
    "lint:libs": "./scripts/lint.sh",
    "commit": "npx git-cz",
    "semver": "lerna version --yes --conventional-commits --message 'chore(release): publish %v'",
    "deploy": "./scripts/deploy.sh",
    "postinstall": "cp ./scripts/commit-msg .git/hooks && ngcc --tsconfig './tsconfig.base.json'",
    "embedme": "embedme",
    "readme": "npm run embedme projects/ctng/testing/README.md",
    "readme:verify": "npm run readme -- -- --verify"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.6",
    "@angular/cdk": "10.1.1",
    "@angular/common": "10.0.6",
    "@angular/compiler": "10.0.6",
    "@angular/core": "10.0.6",
    "@angular/forms": "10.0.6",
    "@angular/material": "10.1.1",
    "@angular/platform-browser": "10.0.6",
    "@angular/platform-browser-dynamic": "10.0.6",
    "@angular/router": "10.0.6",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "angular-in-memory-web-api": "0.10.0",
    "core-js": "2.5.7",
    "crypto-js": "3.3.0",
    "jwt-decode": "2.2.0",
    "ngx-markdown": "10.1.0",
    "rxjs": "6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular-devkit/build-ng-packagr": "~0.1000.4",
    "@angular/cli": "10.0.4",
    "@angular/compiler-cli": "10.0.6",
    "@angular/language-service": "10.0.6",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "12.11.1",
    "codelyzer": "^6.0.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "embedme": "1.20.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "lerna": "3.4.3",
    "lodash.clonedeep": "4.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "8.6.2",
    "tslint": "~6.1.0",
    "typescript": "3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
